Id(\"note-text\").innerText;\n    let newNotesObj = {};\n    if (notes) {\n      newNotesObj = notes;\n    }\n    newNotesObj[huh_user] = newnote;\n    Firebase.database().ref(`/${username}`).set(newNotesObj);\n    setIsVisible(false);\n  };\n  return (\n    <AnimatePresence>\n      {isVisible && (\n        <motion.div className=\"overlay-container\">\n          <motion.div\n            className=\"overlay-bg\"\n            variants={overlayBGVariants}\n            initial=\"hidden\"\n            animate=\"visible\"\n            exit=\"hidden\"\n            onClick={() => {\n              handleSubmit();\n              // toggle();\n            }}\n          ></motion.div>\n          <motion.div\n            variants={overlayContentContainerVariants}\n            initial=\"hidden\"\n            animate=\"visible\"\n            exit=\"hidden\"\n            className=\"overlay-content-container\"\n          >\n            <motion.div\n              variants={overlayContentVariants}\n              className=\"overlay-content\"\n            >\n              <motion.h1>{displayname}</motion.h1>\n              <motion.p>{username}</motion.p>\n              <Editor\n                notes={\n                  notes\n                    ? notes[huh_user]\n                      ? notes[huh_user].length > 0\n                        ? notes[huh_user]\n                        : null\n                      : null\n                    : null\n                }\n              />\n            </motion.div>\n          </motion.div>\n        </motion.div>\n      )}\n    </AnimatePresence>\n  );\n};\n\nexport default Overlay;\n","/Users/shivanku.kumar/Documents/Personal/huh/huh_ext_with_app/src/Components/Editor.js",[],"/Users/shivanku.kumar/Documents/Personal/huh/huh_ext_with_app/src/firebaseConfig.js",[],{"ruleId":"35","replacedBy":"36"},{"ruleId":"37","replacedBy":"38"},{"ruleId":"39","severity":1,"message":"40","line":59,"column":6,"nodeType":"41","endLine":59,"endColumn":32,"sugge