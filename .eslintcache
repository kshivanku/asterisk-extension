lse;\n      }\n    }\n  };\n  return (\n    <AnimatePresence>\n      {isVisible && (\n        <motion.div className=\"overlay-container\">\n          <motion.div\n            className=\"overlay-bg\"\n            variants={overlayBGVariants}\n            initial=\"hidden\"\n            animate=\"visible\"\n            exit=\"hidden\"\n            onClick={() => {\n              handleSubmit();\n              // toggle();\n            }}\n          ></motion.div>\n          <motion.div\n            variants={overlayContentContainerVariants}\n            initial=\"hidden\"\n            animate=\"visible\"\n            exit=\"hidden\"\n            className=\"overlay-content-container\"\n          >\n            <motion.div\n              variants={overlayContentVariants}\n              className=\"overlay-content\"\n            >\n              <header>\n                <div>\n                  <motion.h1>{displayname}</motion.h1>\n                  <motion.p>\n                    <a href={`https://twitter.com/${huh_user}`}>@{username}</a>\n                  </motion.p>\n                </div>\n                <DoneButton handleSubmit={handleSubmit} />\n              </header>\n              <Editor\n                notes={\n                  notes\n                    ? notes[huh_user]\n                      ? notes[huh_user].length > 0\n                        ? notes[huh_user]\n                        : null\n                      : null\n                    : null\n                }\n              />\n              {publicNotesExit() && (\n                <PublicNotes notes={notes} huh_user={huh_user} />\n              )}\n            </motion.div>\n          </motion.div>\n        </motion.div>\n      )}\n    </AnimatePresence>\n  );\n};\n\nexport default Overlay;\n","/Users/shivanku.kumar/Documents/Personal/huh/huh_ext_with_app/src/Components/Editor.js",[],"/Users/shivanku.kumar/Documents/Personal/huh/huh_ext_with_app/src/firebaseConfig.js",[],"/Users/shivanku.k