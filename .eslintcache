  };\n\n  useEffect(() => {\n    // console.log(displayname, username, huh_user, instanceNumber);\n    let ref = Firebase.database().ref(`/${username}`);\n    ref.on(\"value\", (snapshot) => {\n      setNotes(snapshot.val());\n    });\n    setIsVisible(true);\n  }, [username, instanceNumber]);\n  const handleSubmit = () => {\n    const newnote = document.getElementById(\"note-text\").innerText;\n    let newNotesObj = {};\n    if (notes) {\n      newNotesObj = notes;\n    }\n    newNotesObj[huh_user] = newnote;\n    Firebase.database().ref(`/${username}`).set(newNotesObj);\n    setIsVisible(false);\n  };\n  const publicNotesExit = () => {\n    if (!notes) {\n      return false;\n    } else {\n      let authors = notes ? Object.keys(notes) : [];\n      if (authors.indexOf(huh_user) > -1) {\n        authors.splice(huh_user, 1);\n      }\n      if (authors.length > 0) {\n        return true;\n      } else {\n        return false;\n      }\n    }\n  };\n  return (\n    <AnimatePresence>\n      {isVisible && (\n        <motion.div className=\"overlay-container\">\n          <motion.div\n            className=\"overlay-bg\"\n            variants={overlayBGVariants}\n            initial=\"hidden\"\n            animate=\"visible\"\n            exit=\"hidden\"\n            onClick={() => {\n              handleSubmit();\n              // toggle();\n            }}\n          ></motion.div>\n          <motion.div\n            variants={overlayContentContainerVariants}\n            initial=\"hidden\"\n            animate=\"visible\"\n            exit=\"hidden\"\n            className=\"overlay-content-container\"\n          >\n            <motion.div\n              variants={overlayContentVariants}\n              className=\"overlay-content\"\n            >\n              <motion.h1>{displayname}</motion.h1>\n              <motion.p>\n                <a href={`https://twitter.com/${huh_user}`}>@{username}</a>\n              </motion.p>\n              <Editor\n                notes={\n                  notes\n                    ? notes[huh_user]\n                      ? notes[huh_user].length > 0\n                        ? notes[huh_user]\n                        : null\n                      : null\n                    : null\n                }\n              />\n              {publicNotesExit() && (\n                <PublicNotes notes={notes} huh_user={huh_user} />\n              )}\n            </motion.div>\n          </motion.div>\n        </motion.div>\n      )}\n    </AnimatePresence>\n  );\n};\n\nexport default Overlay;\n","/Users/shivanku.kumar/Documents/Personal/huh/huh_ext_with_app/src/Components/Editor.js",[],"/Users/shivanku.kumar/Documents/Personal/huh/huh_ext_with_app/src/firebaseConfig.js",[],"/Users/shivanku.kumar/Documents/Personal/huh/huh_ext_with_app/src/Components/PublicNotes.js",[],{"ruleId":"39","replacedBy":"40"},{"ruleId":"41","replacedBy":"42"},{"ruleId":"43","severity":1,"message":"44","line":60,"column":6,"nodeType":"45","endLine":60,"endColumn":32,"suggestions":"46"},"no-native-reassign",["47"],"no-negated-in-lhs",["48"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'Firebase'. Either include it or remove the dependency array.","ArrayExpression",["49"],"no-global-assign","no-unsafe-negation",{"desc":"50","fix":"51"},"Update the dependencies array to be: [username, instanceNumber, Firebase]",{"range":"52","text":"53"},[1391,1417],"[username, instanceNumber, Firebase]"]