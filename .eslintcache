ainerVariants = {\n    hidden: {\n      y: 500,\n      transition: {\n        // when: \"afterChildren\",\n        duration: 0.2,\n        ease: [0.43, 0.13, 0.23, 0.96],\n      },\n    },\n    visible: {\n      y: 0,\n      transition: {\n        // when: \"beforeChildren\",\n        delayChildren: 0.2,\n        duration: 0.3,\n        ease: [0.43, 0.13, 0.23, 0.96],\n      },\n    },\n  };\n\n  const overlayContentVariants = {\n    hidden: { opacity: 0, y: 10 },\n    visible: {\n      opacity: 1,\n      y: 0,\n      transition: {\n        duration: 0.2,\n        ease: [0.43, 0.13, 0.23, 0.96],\n      },\n    },\n  };\n\n  useEffect(() => {\n    // console.log(displayname, username, huh_user, instanceNumber);\n    let ref = Firebase.database().ref(`/${username}`);\n    ref.on(\"value\", (snapshot) => {\n      setNotes(snapshot.val());\n    });\n    setIsVisible(true);\n  }, [username, instanceNumber]);\n  const handleSubmit = () => {\n    const newnote = document.getElementById(\"note-text\").innerText;\n    let newNotesObj = {};\n    if (notes) {\n      newNotesObj = notes;\n    }\n    newNotesObj[huh_user] = newnote;\n    Firebase.database().ref(`/${username}`).set(newNotesObj);\n    setIsVisible(false);\n  };\n  const publicNotesExit = () => {\n    if (!notes) {\n      return false;\n    } else {\n      let authors = notes ? Object.keys(notes) : [];\n      if (authors.indexOf(huh_user) > -1) {\n        authors.splice(huh_user, 1);\n      }\n      if (authors.length > 0) {\n        return true;\n      } else {\n        return false;\n      }\n    }\n  };\n  return (\n    <AnimatePresence>\n      {isVisible && (\n        <motion.div className=\"overlay-container\">\n          <motion.div\n            className=\"overlay-bg\"\n            variants={overlayBGVariants}\n            initial=\"hidden\"\n            animate=\"visible\"\n            exit=\"hidden\"\n            onClick={() => {\n              handleSubmit();\n              // toggle();\n            }}\n          ></motion.div>\n          <motion.div\n            variants={overlayContentContainerVariants}\n            initial=\"hidden\"\n            animate=\"visible\"\n            exit=\"hidden\"\n            className=\"overlay-content-container\"\n          >\n            <motion.div\n              variants={overlayContentVariants}\n              className=\"overlay-content\"\n            >\n              <motion.h1>{displayname}</motion.h1>\n              <motion.p>\n                <a href={`https://twitter.com/${huh_user}`}>@{username}</a>\n              </motion.p>\n              <Editor\n                notes={\n                  notes\n                    ? notes[huh_user]\n                      ? notes[huh_user].length > 0\n                        ? notes[huh_user]\n                        : null\n                      : null\n                    : null\n                }\n              />\n              {publicNotesExit() && (\n                <PublicNotes notes={notes} huh_user={huh_user} />\n              )}\n            </motion.div>\n          </motion.div>\n        </motion.div>\n      )}\n    </AnimatePresence>\n  );\n};\n\nexport default Overlay;\n","/Users/shivanku.kumar/Documents/Personal/huh/huh_ext_with_app/src/Components/Editor.js",[],"/Users/shivanku.kumar/Documents/Personal/huh/huh_ext_with_app/src/firebaseConfig.js",[],"/Users/shivanku.kumar/Documents/Personal/huh/huh_ext_with_app/src/Components/PublicNotes.js",[],["42","43"],{"ruleId":"44","replacedBy":"45"},{"ruleId":"46","replacedBy":"47"},{"ruleId":"48","severity":1,"message":"49","line":1,"column":8,"nodeType":"50","messageId":"51","endLine":1,"endColumn":12},{"ruleId":"52","severity":1,"message":"53","line":60,"column":6,"nodeType":"54","endLine":60,"endColumn":32,"suggestions":"55"},{"ruleId":"44","replacedBy":"56"},{"ruleId":"46","replacedBy":"57"},"no-native-reassign",["58"],"no-negated-in-lhs",["59"],"no-unused-vars","'logo' is defined but never used.","Identifier","unusedVar","re